@model doorserve.Models.EmployeeModel
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}



<div class="panel-heading">
    <h3 class="m-y-0">Edit Engineer</h3>
</div>

<div class="panel-body">
    <!--/.col-->
    @using (Html.BeginForm("Edit", "Employees", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.HiddenFor(x => x.EmpId)
        @Html.HiddenFor(x => x.EMPPhoto)
        @Html.Partial("_CreateEditEmployeeForm", Model)
        <div class="form-group row">
            <div class="col-sm-2 col-md-2 ">
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
           </div>
            }
        </div>
        @section scripts{
            @Scripts.Render("~/bundles/jqueryval")
        }
