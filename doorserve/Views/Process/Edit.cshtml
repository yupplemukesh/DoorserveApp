@model doorserve.Models.ProcessModel
@{
    /**/

    /**/

    ViewBag.Title = "Edit Process";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="panel-heading">

    <h3 class="m-y-0">Edit Process</h3>

</div>
@using (Html.BeginForm("Edit", "Process", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="panel-body">
        @Html.HiddenFor(x => x.ProcessId)
        @Html.Partial("_CreateEditProcess", Model)
    </div>
    <div class="row">
        <div class="col-sm-5 col-md-5 ">
            <button class="btn btn-primary pull-right">SAVE</button>
            </div>
        </div>
            }
            @section scripts{
                @Scripts.Render("~/bundles/jqueryval")
            }
