@model IEnumerable<TogoFogo.Models.ServiceOfferedModel>
@using TogoFogo.Models
@{
    ViewBag.Title = "Serviceable Area Pin Code";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="panel-heading">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-6">
                <h3 class="m-y-0">Serviceable Area Pincode</h3>
            </div>
            <div class="col-md-6 ">
                <div class="pull-right">
                    @if (Permissions.Rights.ExcelExport)
                    {
                        <button class="btn btn-secondary excel">
                            Export Excel
                        </button>
                    }
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel-body">
    <div class="row">
        <div class="col-md-12">
            <div class="x_content" style="overflow-x: scroll;">
                @Html.Partial("~/views/common/_ServiceableAreaPinTable.cshtml", Model)
            </div>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(".action").hide();
        </script>

    }