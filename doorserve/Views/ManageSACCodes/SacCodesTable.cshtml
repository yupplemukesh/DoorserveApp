@model IEnumerable<doorserve.Models.SacCodesModel>
@{
    Layout = null;
    int Count = 1;
}
<div class="panel panel-default panel-table">
    <div class="panel-heading">
        <div class="row">
            <div class="col-lg-12">
                <div class="col-md-6">
                    <h3 class="m-y-0">List Of GST HSN Codes</h3>
                </div>
                <div class="col-md-6 ">
                    <div class="pull-right">
                      
                            <button class="btn btn-primary excel">Export to Excel</button>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
    <table class="table table-striped table-bordered table-condensed" id="myTable">
        <thead>
            <tr>
                <th>
                    @Html.DisplayName("Serial No")
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cnty_Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.St_Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Applicabletax)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Gstcategory)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GstHeading)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Gst_HSN_Code)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CTH_Number)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SAC)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Product_Sale_Range)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CGST)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SGST_UTGST)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IGST)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GstProductCat)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GstProductSubCat)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Description_Of_Goods)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.Comments)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsActive)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreatedBy)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreatedDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ModifyBy)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ModifyDate)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.DeleteBy)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DeleteDate)
                    </th>*@
                <th>Actions</th>
            </tr>
        </thead>


        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(ModelItem=>item.SerialNo)</td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cnty_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.St_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Applicabletax)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gstcategory)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GstHeading)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gst_HSN_Code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CTH_Number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SAC)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Product_Sale_Range)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CGST)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SGST_UTGST)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IGST)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GstProductCat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GstProductSubCat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description_Of_Goods)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Comments)
                </td>
                <td>
                    @*@Html.DisplayFor(modelItem => item.IsActive)*@
                    @(item.IsActive == true ? "Active" : "InActive")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cby)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mby)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ModifyDate)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.DeleteBy)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DeleteDate)
                    </td>*@
                <td style="display: inline-flex">
                    @* @Url.Action("#Editmodel", new { id = item.BrandId })*@

                    <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize " onclick="EditSAC(@item.SacCodesId)">Edit</a>


                    @*<a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>


        <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />*@

                </td>
                    </tr>
                }

            </table>
        </div>
