@model TogoFogo.Models.ClientDataModel
<div class="panel-body">


    <div class="form-horizontl">
        <h4 class="m-t-0 m-b-25 p-t-10 p-b-10">Call Information :</h4>

        <div class="form-group row">
            <div class="col-sm-2"> <h5 class="m-t-0">Service Type</h5></div>
            <div class="col-sm-10">
                @foreach (var item in Model.ServiceTypesList)
                {
                    <label class="custom-control custom-control-primary custom-checkbox">
                        @Html.RadioButtonFor(x => x.ServiceTypeId, item.Value)
                        <span>@item.Text</span>
                    </label>
                }

            </div>
            @Html.ValidationMessageFor(x => x.ServiceTypeId, "", new { @class = "text-danger" })
        </div>

        <div class="form-group row">
            <div class="col-sm-2"> <h5 class="m-t-0">Service Delivery Type</h5></div>
            <div class="col-sm-10">
                @foreach (var item in Model.DeliveryTypesList)
                {
                    <label class="custom-control custom-control-primary custom-checkbox">
                        @Html.RadioButtonFor(x => x.DeliveryTypeId, item.Value)
                        <span>@item.Text</span>
                    </label>
                }

            </div>
            @Html.ValidationMessageFor(x => x.DeliveryTypeId, "", new { @class = "text-danger" })


        </div>

    </div>



</div>


