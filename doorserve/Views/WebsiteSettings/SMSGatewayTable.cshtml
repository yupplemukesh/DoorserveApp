
@model IEnumerable<doorserve.Models.SMSGateway>
@{
    Layout = null;
}


<table class="table table-striped table-bordered table-condensed" id="myTable">
    <thead>
        <tr>


            <th>
                @Html.DisplayNameFor(model => model.ProcessName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SettingName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginURL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Paswrd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.URLSetting)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SuccessMsg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AvailCredit)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comments)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifyBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifyDate)
            </th>

            <th>Actions</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.ProcessName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SettingName)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginURL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Paswrd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.URLSetting)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SuccessMsg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AvailCredit)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyDate)
            </td>

            <td style="display: inline-flex">
                @* @Url.Action("#Editmodel", new { id = item.BrandId })*@
                <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize " onclick="Edit(@item.SettingID)">Edit</a>
                <a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>
                <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />
            </td>
        </tr>
    }

</table>





