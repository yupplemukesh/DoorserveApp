
@model doorserve.Models.AllData
@{
	Layout = null;
}


<div class="row">
	<div class="col-md-7">
		@Html.LabelFor(model => model.CC_NO, htmlAttributes: new { @class = "col-md-5 col-md-offset-1 col-xs-12" })
		<div class="col-md-6 col-sm-6 col-xs-12">
			@Html.TextBoxFor(model => model.CC_NO, htmlAttributes: new { @class = "form-control" })
			@Html.ValidationMessageFor(model => model.CC_NO, "", new { @class = "text-danger" })
		</div>
	</div>
	<div class="col-md-3 col-md-offset-1">
		<button type="button" class="btn btn-info" onclick="CCno(document.getElementById('CC_NO').value)">Search</button>
	</div>
</div>
<br />








