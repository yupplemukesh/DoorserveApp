@model doorserve.Models.ManageTRCModel
@{
    Layout = null;
}
<div class="panel panel-default panel-table">
    <div class="panel-heading">
        <div class="row">
            <div class="col-lg-12">
                <div class="col-md-6">
                    <h3 class="m-y-0">List Of TRC</h3>
                </div>
                <div class="col-md-6 ">
                    <div class="pull-right">
                        @if (Model._UserActionRights.ExcelExport)
                        {
                            <button class="btn btn-primary excel">Export to Excel</button>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>

    <table class="table table-striped table-bordered table-condensed" id="myTable">
        <thead>
            <tr>
                <th>
                    @Html.DisplayName("Serial No")
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PROCESS_NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TRC_CODE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TRC_NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ORG_NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PIN_CODE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.STATE_TERRITORY)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CITY)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FIRST_NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LAST_NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MOBILE_NO)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EMAIL)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.COMMENTS)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsActive)
                </th>

                <th>
                    @Html.DisplayName("Created By")
                </th>
                <th>
                    @Html.DisplayName("Modify By")
                </th>
                <th>
                    @Html.DisplayName("Modify Date")
                </th>
                <th>Action</th>
            </tr>
        </thead>
        @foreach (var item in Model.ManageTRCModelList)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SerialNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PROCESS_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TRC_CODE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TRC_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ORG_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PIN_CODE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.STATE_TERRITORY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LOCALITY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FIRST_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LAST_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MOBILE_NO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EMAIL)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.COMMENTS)
                </td>
                <td>
                    
                    @(item.IsActive == true ? "Active" : "InActive")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CREATE_BY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MODIFY_BY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MODIFY_DATE)
                </td>
                <td style="display: inline-flex">
                    @* @Url.Action("#Editmodel", new { id = item.BrandId })*@
                    @*<a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>
                      <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />*@
                    @if (Model._UserActionRights.Edit)
                    {
                        <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize " onclick="EditTRc('@item.TRC_ID')">Edit</a>
                    }
                    </td>
            </tr>
        }

    </table>

</div>

