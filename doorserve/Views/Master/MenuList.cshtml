@model IEnumerable<doorserve.Models.TableMenuModel>

<table class="table table-striped table-bordered table-condensed" id="test1">
    <thead>

        <tr>
            <th hidden>
                @Html.DisplayNameFor(model => model.MenuCap_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Menu_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubMenuName)
            </th>
            <th hidden>
                @Html.DisplayNameFor(model => model.ParentMenuID)
            </th>
        
            <th> View Status</th>
        </tr>

    </thead>
    <tbody>


        @foreach (var item in Model)
        {
           
            <tr>
                <td hidden>@Html.DisplayFor(modelItem => item.MenuCap_ID)</td>
                <td>@Html.DisplayFor(modelItem => item.Menu_Name)</td>
                <td>@Html.DisplayFor(modelItem => item.SubMenuName)</td>
                <td hidden>@Html.DisplayFor(modelItem => item.ParentMenuID)</td>
                <td>@Html.CheckBoxFor(modelItem => item.isNewlyEnrolled)</td>
            </tr>
        }
    </tbody>
</table>
