@model doorserve.Models.ActionTypeModel
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}
<div class="panel-heading">

    <h3 class="m-y-0">Edit ActionType</h3>
</div>
@using (Html.BeginForm("Edit", "ActionTypes", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(x => x.ActionTypeId)
    <!--/.row-->
  
     
    <div class="panel-body">


        @Html.Partial("_CreateEditActionTypesForm", Model)
        <div class=" row form-group">
            <div class="pull-right">
                <button class="btn btn-secondary">
                    Reset
                </button>
                <button class="btn btn-primary">
                    Save
                </button>

            </div>
        </div>

    </div>
}



@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
}