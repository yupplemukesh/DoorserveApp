@model IEnumerable<TogoFogo.Models.CourierPinZipCode>
@{
    Layout = null;
}


<table class="table table-striped table-bordered table-condensed" id="myTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Pin Zip Id")
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Courier)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PIN_Country1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PIN_State1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PIN_City1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pin_Region)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Pin_Zone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pin_Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pin_TAT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pin_Cod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShortCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ISExpress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReverseLogistics)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderPreference)
            </th>

            <th>Action</th>
        </tr>
    </thead>
    

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pin_ZIP_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Courier)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PIN_Country1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PIN_State1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PIN_City1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pin_Region)
        </td>


        <td>
            @Html.DisplayFor(modelItem => item.Pin_Zone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pin_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pin_TAT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pin_Cod)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShortCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ISExpress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReverseLogistics)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderPreference)
        </td>

        <td style="display: inline-flex">

            <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize " onclick="EditPinZipCode(@item.Pin_ZIP_ID)">Edit</a>
            <a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>
            <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />
        </td>
    </tr>
    }

</table>
