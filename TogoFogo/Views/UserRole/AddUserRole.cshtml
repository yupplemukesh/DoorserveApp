@model TogoFogo.Models.UserRole
@{
    ViewBag.Title = "Add User Role";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

@section Styles{
    @Styles.Render("~/Content/plugintree")
}
<div class="panel-heading">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-6">
                <h3 class="m-y-0">Add User Role</h3>
            </div>
        </div>
    </div>
</div>
<div class="panel-body">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_content">
                @using (Html.BeginForm("AddUserRole", "UserRole", FormMethod.Post, new { id = "form", enctype = "multipart/form-data" }))
                {
                    @Html.Partial("_ManageUserRole", Model)
                }
            </div>
        </div>
    </div>
</div>


@section scripts{
    @Scripts.Render("~/bundles/appjs")
}