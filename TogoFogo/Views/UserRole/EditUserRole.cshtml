@model TogoFogo.Models.UserRole
@{
    ViewBag.Title = "Edit User Role";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}
<div class="panel-heading">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-6">
                <h3 class="m-y-0">Edit User Role</h3>
            </div>
        </div>
    </div>
</div>
<div class="panel-body">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">

            <div class="x_content">
                @using (Html.BeginForm("EditUserRole", "UserRole", FormMethod.Post, new { id = "form", enctype = "multipart/form-data" }))
                {
                    @Html.HiddenFor(x=>x.RoleId)
                    @Html.Partial("_ManageUserRole", Model)
                }
            </div>
        </div>
    </div>
</div>


