@model IEnumerable<TogoFogo.Models.ServiceChargeModel>
@{
	Layout = null;													
}


<table class="table table-striped table-bordered table-condensed" id="myTable">
	<thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.CatName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubCatName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BrandName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModelName)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.HSNCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SAC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TRUPC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Form)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MRP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MarketPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceCharge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifyDate)
            </th>
            <th>Action</th>
        </tr>
	</thead>
	@foreach (var item in Model)
	{
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.CatName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubCatName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrandName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModelName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HSNCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SAC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TRUPC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Form)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MRP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MarketPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceCharge)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifyDate)
        </td>

        <td style="display: inline-flex;">
            @* @Url.Action("#Editmodel", new { id = item.BrandId })*@
            <a href="#Editmodel" data-toggle="modal" class="btn btn-info btnSize " onclick="EditServiceCharge(@item.ServiceChargeId)">Edit</a>
            <a href="#Historymodel" data-toggle="modal" class="btn btn-success btnSize ">History</a>
            <input type="button" value="Delete" class="btn btn-danger btnSize " />
        </td>
    </tr>
	}

</table>
