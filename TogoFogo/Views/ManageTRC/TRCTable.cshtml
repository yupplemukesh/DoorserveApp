@model IEnumerable<TogoFogo.Models.ManageTRCModel>
@{
    Layout = null;
}


<table class="table table-striped table-bordered table-condensed" id="myTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PROCESS_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TRC_CODE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TRC_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ORG_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PIN_CODE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.STATE_TERRITORY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CITY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FIRST_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LAST_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MOBILE_NO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EMAIL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IS_ACTIVE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.COMMENTS)
            </th>
           
            <th>Actions</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PROCESS_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TRC_CODE)
            </td>
            <td>
    
                    @Html.DisplayFor(modelItem => item.TRC_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ORG_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PIN_CODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STATE_TERRITORY)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LOCALITY)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FIRST_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LAST_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MOBILE_NO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EMAIL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IS_ACTIVE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COMMENTS)
            </td>
            
            <td style="display: inline-flex">
                @* @Url.Action("#Editmodel", new { id = item.BrandId })*@
                <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize "onclick="EditTRc('@item.TRC_ID')">Edit</a>
                @*<a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>
                <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />*@
            </td>
        </tr>
    }

</table>



