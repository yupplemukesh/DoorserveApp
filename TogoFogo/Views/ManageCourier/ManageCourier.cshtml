@model  IEnumerable<TogoFogo.Models.ManageCourierModel>
@{
    ViewBag.Title = "ManageCourier";
}

<div class="panel-heading">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-6">
                <h3 class="m-y-0">List Of Courier</h3>
            </div>  
            <div class="col-md-6 ">
                <div class="pull-right">
                    <button class="btn btn-success" style="text-decoration-color:white">@Html.ActionLink("New Courier", "Create", "ManageCourier")</button>
                    <button class="btn btn-primary excel">Export to Excel</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel-body">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
             
                <div class="x_content" style="overflow-x: scroll;">
                    @Html.Partial("CourierTable", Model)
                </div>
            </div>
        </div>
    </div>
</div>
   

    
