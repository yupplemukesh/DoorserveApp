@model IEnumerable<TogoFogo.Models.ManageServiceProviderModel>
@{
    Layout = null;
}
<table class="table table-striped table-bordered table-condensed" id="myTable">
<thead>
    <tr>
        <th>
            @Html.DisplayName("Serial No")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProcessName)
        </th>
       

        <th>
            @Html.DisplayNameFor(model => model.ProviderName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganisationName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Country)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.St_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LocationName)
        </th>
        
        <th>
            @Html.DisplayName("Name")
        </th>
        <th>
            @Html.DisplayName("Is Active")
        </th>
        <th>
            @Html.DisplayName("Created By")
        </th>
        <th>
            @Html.DisplayName("Modify By")
        </th>
        <th>
            @Html.DisplayName("Created Date")
        </th>
        <th>
            @Html.DisplayName("Modify Date")
        </th>
        <th>Action</th>
    </tr>
</thead>
@foreach (var item in Model)
{
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.SerialNo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ProcessName)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.ProviderName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OrganisationName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AddressType)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Address)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Country)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.St_Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LocationName)
    </td>
    <td>
        @(item.FirstName + " " + item.LastName)

    </td>
    <td>
        @(item.IsActive == true ? "Active" : "InActive")
    </td>



    <td>
        @Html.DisplayFor(modelItem => item.CreatedBy)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ModifyBy)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CreatedDate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ModifyDate)
    </td>
    <td style="display: inline-flex">
        @* @Url.Action("#Editmodel", new { id = item.BrandId })*@
        <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize " onclick="EditServiceProvider(@item.ProviderId)">Edit</a>
        @*<a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>*@
        <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />
    </td>
</tr>
}

</table>
@*  *@