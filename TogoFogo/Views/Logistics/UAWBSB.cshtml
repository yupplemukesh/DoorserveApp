
@model IEnumerable<TogoFogo.Models.ReverseAWB_AllocationModel>


<div class="row">

    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Update Reverse AWB Status (Biker)<small></small></h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>

                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">

                <table class="table table-striped table-bordered table-condensed" id="myTable">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.CC_NO))
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Customer_Name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Mobile_No)
                            </th>

                            <th>Action</th>
                        </tr>
                    </thead>


                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.CC_NO)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.Customer_Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Mobile_No)
                            </td>

                            <td>

                                @Html.ActionLink("Update", "UAWBSBForm", new { }, new { @class = "btn btn-sm btn-size btn-primary" })
                            </td>
                        </tr>
                    }

                </table>
            </div>
        </div>
    </div>
</div>

