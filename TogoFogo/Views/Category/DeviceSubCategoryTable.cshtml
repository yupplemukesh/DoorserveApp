
@model IEnumerable<TogoFogo.Models.SubcategoryModel>
@{
    Layout = null;
}


<table class="table table-striped table-bordered table-condensed" id="myTable">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayName("Device Category")
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubCatName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SortOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IMEI1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IMEI2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IMEI_Length)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sr_no_req)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sr_No_Length)
            </th>

            <th>Actions</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.CatName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubCatName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SortOrder)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IMEI1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IMEI2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IMEI_Length)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sr_no_req)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sr_No_Length)
            </td>

            <td style="display: inline-flex">
                @* @Url.Action("#Editmodel", new { id = item.BrandId })*@
                <a href="#Editmodel" data-toggle="modal" class="btn btn-small btn-info btnSize " onclick="EditSubCategory(@item.SubCatId)">Edit</a>
                <a href="#Historymodel" data-toggle="modal" class="btn btn-small btn-success btnSize ">History</a>
                <input type="button" value="Delete" class="btn btn-small btn-danger btnSize" />
            </td>
        </tr>
    }

</table>



