@model TogoFogo.Models.WildCardModel
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="panel-heading">

    <h3 class="m-y-0">Edit WildCard</h3>
</div>
@using (Html.BeginForm("Edit", "WildCards", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(x => x.WildCardId)
    <!--/.row-->


    <div class="panel-body">


        @Html.Partial("_CreateEditWildCardsForm", Model)
        <div class=" row form-group">
            <div class="col-md-7 pull-right ">            
                <button class="btn btn-primary">
                    Save
                </button>

            </div>
        </div>

    </div>
}



@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
}


