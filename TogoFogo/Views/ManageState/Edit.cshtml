@model TogoFogo.Models.ManageStateModel
@{
    /**/

    /**/

    ViewBag.Title = "Edit State";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="panel-heading">

    <h3 class="m-y-0">Edit Country</h3>

</div>
@using (Html.BeginForm("Edit", "ManageState", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="panel-body">
        @Html.HiddenFor(x => x.St_ID)
        @Html.Partial("_CreateEditState", Model)
    </div>
    <div class="row">
        <div class="col-sm-5 col-md-5  col-md-offset-2">
            <button class="btn btn-primary">SAVE</button>
        </div>
    </div>
}
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
}

