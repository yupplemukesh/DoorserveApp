@model TogoFogo.Models.BankDetailModel



<div class="col-md-3">

    <div class="row edit-padd">
        <div class="col-md-6 col-xs-6 col-sm-6">
            <h4>
                Bank Details
            </h4>
        </div>
        <div class="col-md-6 col-xs-6 col-sm-6 text-right ">
            <a class="btn btn-sm btn-primary" href="#BankModel" data-toggle="modal" onclick='AddOrEditBank(@Html.Raw(Json.Encode(Model)))'> Edit</a>

        </div>
    </div>

    <div class="row edit-padd">
        <div class="col-md-6 col-xs-6 col-sm-6"> <strong> Name</strong>:</div>
        <div class="col-md-6 col-xs-6 col-sm-6 edit-p">  @Model.BankName</div>

    </div>

    <div class="row edit-padd">
        <div class="col-md-6 col-xs-6 col-sm-6"> <strong> IFSC</strong>: </div>
        <div class="col-md-6 col-xs-6 col-sm-6 edit-p">@Model.BankIFSCCode</div>

    </div>

    <div class="row edit-padd">
        <div class="col-md-6 col-xs-6 col-sm-6"><strong>Account Number </strong>:</div>
        <div class="col-md-6 col-xs-6 col-sm-6 edit-p"> @Model.BankAccountNumber</div>
    </div>

   

</div>



