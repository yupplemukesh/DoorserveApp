@model IList<TogoFogo.Models.SMSGatewayModel>
@{var count = 1;}



        <div class="table-responsive">
            <table class="table table-striped table-bordered dataTable" id="table-1">
                <thead>
                    <tr>
                        <th>SL No</th>
                        <th>GatewayName</th>
                        <th>IsActive</th>
                        <th>Action</th>

                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>@count</td>
                        <td>@item.GatewayName</td>
                        <td>@item.IsActive</td>
                        <td></td>

                    </tr>
                    }
                  

                   


                </tbody>

            </table>
        </div>


