@model  IEnumerable<TogoFogo.Models.CustomerServiceRecord.VisitorsDetailsModel>
@using TogoFogo.Models
@{
                /**/

                Layout = null;
}
<table class="table table-striped table-bordered table-condensed" id="myTable">
    <thead>
        <tr>
            <th>Serial No</th>
            <th>
                @Html.DisplayNameFor(model => model.VisitDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Engineer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OutTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CrNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>         
            
            
            <th>Actions</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VisitDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Engineer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OutTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CrNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>       
           
            <td style="display: inline-flex">
                <a href='#' onclick="getItems('@item.VisitorNo','JsonGetPerson')" class="btn btn-small btn-info btnSize ">Edit</a>
            </td>
        </tr>
    }

</table>
