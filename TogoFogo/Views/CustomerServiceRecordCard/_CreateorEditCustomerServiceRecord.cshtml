                        @Html.TextBoxFor(x => x._Contact.CityId, new { @class = "form-control" })
                        @Html.ValidationMessageFor(x => x._Contact.CityId, "", new { @class = "text-danger" })
                    </div>
                    <label class="col-sm-1 control-label" for="form-control-1">Pincode</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x._Contact.PinNumber, new { @class = "form-control" })
                        @Html.ValidationMessageFor(x => x._Contact.PinNumber, "", new { @class = "text-danger" })
                    </div>

                    <label class="col-sm-2 control-label" for="form-control-1">Contact No.</label>
                    <div class="col-sm-2">
                        @Html.TextBoxFor(x => x._Contact.ConMobileNumber, new { @class = "form-control" })
                        @Html.ValidationMessageFor(x => x._Contact.ConMobileNumber, "", new { @class = "text-danger" })
                    </div>
                </div>



                <div class="row">

                    <label class="col-sm-2 control-label" for="form-control-1">Comp./panel/IMEI No.</label>
                    <div class="col-sm-2">
                        @Html.TextBoxFor(x => x.CompNo, new { @class = "form-control" })
                        @Html.ValidationMessageFor(x => x.CompNo, "", new { @class = "text-danger" })
                    </div>


                    <label class="col-sm-2 control-label" for="form-control-1">Appointment Date/Time</label>
                    <div class="col-sm-2">
                        @Html.TextBoxFor(x => x.AppointmentDate, new { @class = "form-control" })
                        @Html.ValidationMessageFor(x => x.AppointmentDate, "", new { @class = "text-danger" })
                    </div>

