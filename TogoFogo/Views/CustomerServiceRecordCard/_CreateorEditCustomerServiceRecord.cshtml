                        <div class="row">
                            <label class="col-sm-1 control-label" for="form-control-1">Email</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(x => x._Contact.ConEmailAddress, new { @class = "form-control" })
                                @Html.ValidationMessageFor(x => x._Contact.ConEmailAddress, "", new { @class = "text-danger" })
                            </div>


                            <label class="col-sm-3 control-label text-right" for="form-control-1">AMC/INSU. No.</label>
                            <div class="col-sm-3">
                                @Html.TextBoxFor(x => x.AMCNo, new { @class = "form-control" })
                                @Html.ValidationMessageFor(x => x.AMCNo, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>

                    <div class="col-md-5">
                        <div class="row">
                            <label class="col-sm-3 col-md-3 control-label text-right" for="form-control-1">AMC Period:</label>

                            <label class="col-sm-2 control-label" for="form-control-1">From</label>
                            <div class="col-sm-3">
                                @Html.TextBoxFor(x => x.AMCFrom, new { @class = "form-control" })
                                @Html.ValidationMessageFor(x => x.AMCFrom, "", new { @class = "text-danger" })
                            </div>

                            <label class="col-sm-1 control-label" for="form-control-1">To</label>
                            <div class="col-sm-3">
                                @Html.TextBoxFor(x => x.AMCTo, new { @class = "form-control" })
                                @Html.ValidationMessageFor(x => x.AMCTo, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                </div>
            </form>
        </div>
    </div>
</div>







