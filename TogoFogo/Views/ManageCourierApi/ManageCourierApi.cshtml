
@model  IEnumerable<TogoFogo.Models.ManageCourierApiModel>
@{
    ViewBag.Title = "ManageCourierApi";
}


<div class="panel-heading">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-6">
                <h3 class="m-y-0">List Of Courier API</h3>
            </div>
            <div class="col-md-6">
                <div class="pull-right">
                    <button class="btn">@Html.ActionLink("New Courier API", "Create", "ManageCourierApi")</button>
                    <button class="btn btn-primary excel">Export to Excel</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel-body">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">

            <div class="x_content" style="overflow-x: scroll;">
                @Html.Partial("CourierAPITable", Model)
            </div>
        </div>
    </div>
</div>

